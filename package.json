{
  "name": "byzantine-config",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "asn1.js": "^5.0.1",
    "axios": "^0.18.1",
    "electron-debug": "^2.2.0",
    "electron-is-dev": "^1.1.0",
    "fabric-client": "1.4.1",
    "fs": "0.0.2",
    "fs-extra": "^7.0.1",
    "grpc": "^1.20.3",
    "js-sha256": "^0.9.0",
    "json2yaml": "1.1.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^4.1.1",
    "react": "^16.8.6",
    "react-bootstrap": "^0.32.4",
    "react-bootstrap-table": "^4.3.1",
    "react-dom": "^16.8.6",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "socket.io": "^2.2.0"
  },
  "engines": {
    "node": ">=6.9.5 <9.0",
    "npm": ">=3.10.10 <7.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^4.2.0",
    "electron-builder": "^20.39.0",
    "electron-rebuild": "^1.8.4",
    "react-scripts": "3.0.0",
    "wait-on": "^3.2.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron --inspect .\"",
    "electron": "electron .",
    "electron-pack": "build -ml",
    "electron-pack-linux": "build -l",
    "preelectron-pack": "yarn build",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.keyholesoftware.byzantine-config",
    "productName": "Byzantine Config",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "images/icons/mac/icon.icns"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "author": "Keyhole Software",
  "homepage": "./",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
