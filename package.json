{
  "name": "byzantine-config",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "asn1.js": "^5.0.1",
    "axios": "^0.16.2",
    "electron-debug": "^2.0.0",
    "electron-is-dev": "^1.0.1",
    "fabric-client": "1.3.0",
    "fs": "0.0.1-security",
    "fs-extra": "^2.0.0",
    "grpc": "^1.20.0",
    "js-sha256": "^0.9.0",
    "json2yaml": "1.1.0",
    "jsonwebtoken": "^7.3.0",
    "log4js": "^0.6.38",
    "react": "^16.6.1",
    "react-bootstrap": "^0.31.0",
    "react-bootstrap-table": "^3.5.1",
    "react-dom": "^16.6.1",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "socket.io": "^2.1.1"
  },
  "engines": {
    "node": ">=6.9.5 <9.0",
    "npm": ">=3.10.10 <7.0"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "electron": "^3.0.10",
    "electron-builder": "^20.33.2",
    "electron-rebuild": "^1.8.2",
    "react-scripts": "2.1.1",
    "wait-on": "^3.2.0"
  },
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "electron": "electron .",
    "electron-pack": "build -ml",
    "electron-pack-linux": "build -l",
    "preelectron-pack": "yarn build",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.keyholesoftware.byzantine-config",
    "productName": "Byzantine Config",
    "files": ["build/**/*", "node_modules/**/*"],
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "images/icons/mac/icon.icns"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "author": "Keyhole Software",
  "homepage": "./",
  "browserslist": [">0.2%", "not dead", "not ie <= 11", "not op_mini all"]
}
